<%- include("include/head.ejs") %>
    <body>
        <h1>This page belong to : @<%= data.name %></h1>

        <button>Follow</button>
        <button>Message</button>

        <p>Followers : <%= data.followers %> &nbsp; &nbsp; &nbsp;
            &nbsp; &nbsp;Following : <%=data.following %></p>
        <hr><hr>

        <% for(post of data.posts){ %>
        <img src="<%= post.image %>" alt="some img">
        <p>Likes : <%= post.likes %> &nbsp; &nbsp; &nbsp;
            &nbsp; &nbsp;Comments : <%= post.comments %></p>
        <% } %>

        <script src="/app.js"></script>
    </body>
</html>
